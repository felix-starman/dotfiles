{
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.tabSize": 2,
    "workbench.iconTheme": "Monokai Pro (Filter Machine) Icons",
    "window.zoomLevel": 1,
    "window.title": "${dirty}${activeEditorShort}${separator}${activeFolderMedium} [${rootName}]",
    "files.watcherExclude": {
        "**/_build/**": true,
        "**/.elixir_ls/**": true
    },
    "files.exclude": {
        "**/_build": true,
        "**/.elixir_ls": true
    },
    "[markdown]": {},
    "elixirLS.mixEnv": "dev",
    "[elixir]": {
        "editor.defaultFormatter": "JakeBecker.elixir-ls"
    },
    "markdown.extension.toc.githubCompatibility": true,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "editor.largeFileOptimizations": false,
    "files.associations": {
        "Jenkinsfile_build": "jenkinsfile",
        "Jenkinsfile_deploy": "jenkinsfile"
    },
    "workbench.colorTheme": "Gruvbox Light (Medium)",
    "editor.fontLigatures": false,
    "editor.formatOnSave": true,
    "editor.wordSeparators": "`~!@#$%^&*()=+[{]}\\|;:'\",.<>?",
    "workbench.startupEditor": "newUntitledFile",
    "elixirLS.dialyzerFormat": "dialyxir_long",
    "editor.minimap.enabled": false,
    "emmet.includeLanguages": {
        "html-eex": "html"
    }
}
